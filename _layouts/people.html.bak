---
layout: default
---

<!-- about.html -->
      <div class="post">
        <header class="post-header">
          <h1 class="post-title">
           {{ page.title }}
          </h1>
          <p class="desc">{{ page.subtitle }}</p>
        </header>

		<hr />
		
		<article>
          {% if page.profile0 -%}
          <div class="profile float-{%- if page.profile0.align == 'center' -%}center{%- else -%}right{%- endif -%}">
            {%- if page.profile0.image %}
              {%- assign profile_image_path = page.profile1.image | prepend: 'assets/img/' -%}

              {% if page.profile0.image_circular %}
                {%- assign profile_image_class = "img-fluid z-depth-1 rounded-circle" -%}
              {% else %}
                {%- assign profile_image_class = "img-fluid z-depth-1 rounded" -%}
              {% endif %}

              {% include figure.html 
              path=profile_image_path 
              class=profile_image_class
              alt=page.profile0.image -%}
            {% endif -%}
          </div>
          {%- endif %}

          <div class="clearfix">
            {{ page.profile0.blabla }}
          </div>

        <hr />
		
        </article>
		
        <article>
          {% if page.profile1 -%}
          <div class="profile float-{%- if page.profile1.align == 'left' -%}left{%- else -%}right{%- endif -%}">
